{
  "name": "jenkins-scanner",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:reset": "rm -rf .next && rm -rf node_modules/.cache && npm cache clean --force && npm run dev",
    "dev:fresh": "rm -rf .next && npm run build && npm run dev",
    "build": "next build",
    "build:clean": "rm -rf .next && npm run build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:integration": "jest --testMatch='**/__tests__/integration/*.test.ts'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "security:scan": "npm audit --audit-level=high",
    "docker:build": "docker build -t jenkins-scanner .",
    "docker:run": "docker run -p 3000:3000 jenkins-scanner"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.535.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "tailwind-merge": "^3.3.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.7.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2"
  }
}
